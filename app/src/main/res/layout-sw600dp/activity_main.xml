<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/soccer_icon"
        android:layout_width="228dp"
        android:layout_height="228dp"
        android:layout_marginTop="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/charging_station"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/charging_station"
        app:layout_constraintTop_toBottomOf="@+id/charging_station"
        app:layout_constraintVertical_bias="0.079"
        app:srcCompat="@drawable/soccer_court_icon" />

    <TextView
        android:id="@+id/bus"
        android:layout_width="198dp"
        android:layout_height="45dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/archivo_black"
        android:gravity="center"
        android:text="@string/bus"
        app:layout_constraintBottom_toTopOf="@+id/movie_icon"
        app:layout_constraintEnd_toEndOf="@+id/bus_icon"
        app:layout_constraintHorizontal_bias="0.214"
        app:layout_constraintStart_toStartOf="@+id/bus_icon"
        app:layout_constraintTop_toBottomOf="@+id/bus_icon"
        app:layout_constraintVertical_bias="0.514" />

    <ImageView
        android:id="@+id/movie_icon"
        android:layout_width="228dp"
        android:layout_height="228dp"
        android:layout_marginTop="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/bus_icon"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/bus_icon"
        app:layout_constraintTop_toBottomOf="@+id/bus_icon"
        app:layout_constraintVertical_bias="0.079"
        app:srcCompat="@drawable/movie_icon" />

    <TextView
        android:id="@+id/movie_info"
        android:layout_width="212dp"
        android:layout_height="42dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/archivo_black"
        android:gravity="center"
        android:text="@string/movie_info"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/movie_icon"
        app:layout_constraintHorizontal_bias="0.187"
        app:layout_constraintStart_toStartOf="@+id/movie_icon"
        app:layout_constraintTop_toBottomOf="@+id/movie_icon"
        app:layout_constraintVertical_bias="0.131" />

    <TextView
        android:id="@+id/soccer_game"
        android:layout_width="162dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/archivo_black"
        android:gravity="center"
        android:text="@string/soccer_game"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/soccer_icon"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="@+id/soccer_icon"
        app:layout_constraintTop_toBottomOf="@+id/soccer_icon"
        app:layout_constraintVertical_bias="0.135" />

    <TextView
        android:id="@+id/car_charging"
        android:layout_width="219dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="288dp"
        android:fontFamily="@font/archivo_black"
        android:gravity="center"
        android:text="@string/car_charging"
        app:layout_constraintBottom_toTopOf="@+id/soccer_icon"
        app:layout_constraintEnd_toEndOf="@+id/charging_station"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/charging_station"
        app:layout_constraintTop_toBottomOf="@+id/charging_station"
        app:layout_constraintVertical_bias="0.447" />

    <ImageView
        android:id="@+id/charging_station"
        android:layout_width="228dp"
        android:layout_height="228dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="396dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.187"
        app:srcCompat="@drawable/charging_station" />

    <ImageView
        android:id="@+id/bus_icon"
        android:layout_width="228dp"
        android:layout_height="228dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/charging_station"
        app:layout_constraintHorizontal_bias="0.671"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.187"
        app:srcCompat="@drawable/bus_icon" />

</androidx.constraintlayout.widget.ConstraintLayout>